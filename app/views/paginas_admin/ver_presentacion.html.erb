
<div class="row">
  <div class="col s12">
<div class="carousel carousel-slider" style= "height:100vh" >
  <%@ppts.each do |slide|%>
    <a class="carousel-item" href="#<%=slide.posicion%>"><%=image_tag slide.imagen.url+"?time="+Time.now.to_i.to_s, class: "", id:"img_prev", style:"height: 90%; max-width: 100%" if slide.imagen?%></a>
  <%end%>
</div>
</div>
</div>


 <script>
 document.addEventListener('DOMContentLoaded', function() {
    var elems = document.querySelectorAll('.carousel');
    var instance = M.Carousel.init(elems,{
    fullWidth: true
  });
  });
 </script>
